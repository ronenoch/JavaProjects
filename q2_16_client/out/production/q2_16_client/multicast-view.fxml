<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>

<VBox alignment="CENTER" spacing="20.0"
      style="-fx-background-color: silver;"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="MulticastController"
      fx:id="v_box">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <TextField fx:id="textF" maxWidth="300.0">
    </TextField>

    <Button text="clear" onAction="#clearMessage"/>
    <Button text="connect" onAction="#connectPressed"/>
    <Button text="leave" onAction="#leavePressed"/>
</VBox>